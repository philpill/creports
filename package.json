{
  "name": "Conflicting Reports",
  "description": "Graphical visualisation of conflicts reported around the world",
  "version": "0.0.1",
  "private": true,
  "ref": [
    "https://github.com/substack/node-browserify#browser-field",
    "https://github.com/thlorenz/browserify-shim#multi-shim-example-including-dependencies"
  ],
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.min.js",
    "topojson": "./bower_components/topojson/topojson.js",
    "d3": "./bower_components/d3/d3.min.js",
    "datamaps": "./bower_components/datamaps/dist/datamaps.all.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "d3": "d3",
    "topojson": "topojson",
    "datamaps": {
      "depends": [
        "d3",
        "topojson"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "dependencies": {
    "jade": "^1.9.1"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "browserify": "^8.1.1",
    "browserify-shim": "^3.2.0",
    "cheerio": "^0.18.0",
    "express": "^4.11.1",
    "gulp": "^3.8.10",
    "gulp-batch": "^1.0.4",
    "gulp-sass": "^1.3.2",
    "gulp-uglify": "^1.1.0",
    "gulp-watch": "^1.0.4",
    "pg": "^4.2.0",
    "q": "^1.1.2",
    "request": "^2.51.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
  }
}
